public class Operations {

	public static void main(String[] args) {
		
	int a = 20;
	int b = 10;
	
	//arithmetic operation
	int sum = a+b;
	System.out.println(sum);
	
	int mul = a*b;
	System.out.println(mul);
	
	//relational operator
	boolean result = 5>2;
			System.out.println(result);

	result = 5>=2;
	System.out.println(result);
	int age = 19; 
	boolean isValidVoterIdCard = false;
	
	result = age>= 18 && isValidVoterIdCard == true;
	
	System.out.println(result);
			
	int marks = 89;
	int attendance = 95;
	result = marks>90 || attendance == 100;
			System.out.println(result);
			result = !(6>4);
			System.out.println(result);
			int sum1 = 10;
			sum1+=10; 
			System.out.println(result);
	}
	
